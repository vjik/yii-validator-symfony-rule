{
    "name": "vjik/yii-validator-symfony-rule",
    "type": "library",
    "description": "Symfony validation constraints adapter for Yii Validator",
    "keywords": [
        "yii",
        "validator",
        "validation",
        "symfony",
        "constraint"
    ],
    "license": "BSD-3-Clause",
    "support": {
        "issues": "https://github.com/vjik/yii-validator-symfony-rule/issues?state=open",
        "source": "https://github.com/vjik/yii-validator-symfony-rule"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^8.0",
        "yiisoft/validator": "3.0.x-dev",
        "symfony/validator": "^6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "roave/infection-static-analysis-plugin": "^1.25",
        "vimeo/psalm": "^4.30|^5.4"
    },
    "autoload": {
        "psr-4": {
            "Vjik\\Yii\\ValidatorSymfonyRule\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Vjik\\Yii\\ValidatorSymfonyRule\\Tests\\": "tests"
        }
    },
    "scripts": {
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "infection/extension-installer": true,
            "composer/package-versions-deprecated": true
        }
    }
}
